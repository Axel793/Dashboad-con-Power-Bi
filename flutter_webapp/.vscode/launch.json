// .vscode/launch.json
{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter • lib/main.dart",
      "request": "launch",
      "type": "dart",
      // AQUÍ indicas el entry-point; para un proyecto Flutter por defecto:
      "main.dart": "lib/main.dart",
      // Opcional: corre en Chrome o dispositivo conectado
      "deviceId": "chrome"          // o "windows", "macos", "android-id", etc.
    }
  ]
}

